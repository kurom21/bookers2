<%= render 'books/list', book: @book %>

<h1>New book</h1>
<%= form_with model: @book, url: books_path, method: :post do |f| %>
 <label>Title</label>
 <%= f.text_field :title %>
 <label>Opinion</label>
 <%= f.text_area :body %>
 <%= f.submit 'Create Book' %>
<% end %>

<h1>Users</h1>
<% @user.each do |user| %>
<label>image</label>
<%= image_tag user.get_profile_image(100,100) %>
<label>name</label>
<%= user.name %>

<%= link_to 'Show', user_path(user.id) %>
<% end %>